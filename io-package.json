{
  "common": {
    "name": "iobroker.pacs",
    "version": "0.0.1",
    "title": "PACS Adapter",
    "desc": "PACS adapter for user access control with event tracking",
    "authors": [
      "Mikhail Kolesnikov <m89139158466@gmail.com>"
    ],
    "keywords": [
      "PACS",
      "access control",
      "events",
      "users",
      "devices"
    ],
    "license": "MIT",
    "platform": "Javascript/Node.js",
    "icon": "admin/icon.png",
    "enabled": true,
    "extIcon": "https://github.com/mihailkol/iobroker.pacs/blob/master/admin/icon.png",
    "readme": "https://github.com/mihailkol/iobroker.pacs/blob/master/README.md",
    "loglevel": "info",
    "mode": "daemon",
    "type": "communication",
    "connectionType": "local",
    "dataSource": "poll",
    "adminUI": {
      "config": "json"
    },
    "compact": true,
    "messagebox": true,
    "dependencies": [
      {
        "js-controller": ">=5.0.19"
      }
    ],
    "globalDependencies": [
      {
        "admin": ">=6.0.0"
      }
    ],
    "tier": 3
  },
  "native": {
    "users": [],
    "devices": [],
    "incomingChannels": []
  },
  "encryptedNative": [],
  "protectedNative": [],
  "objects": [],
  "instanceObjects": [
    {
      "_id": "events",
      "type": "channel",
      "common": {
        "name": "Events",
        "desc": "Channel for storing events"
      },
      "native": {}
    },
    {
      "_id": "events.incoming",
      "type": "state",
      "common": {
        "name": "Incoming events",
        "type": "array",
        "role": "list",
        "read": true,
        "write": true,
        "desc": "Storage for incoming events"
      },
      "native": {}
    },
    {
      "_id": "events.outgoing",
      "type": "state",
      "common": {
        "name": "Outgoing events",
        "type": "array",
        "role": "list",
        "read": true,
        "write": true,
        "desc": "Storage for outgoing events"
      },
      "native": {}
    }
  ]
}
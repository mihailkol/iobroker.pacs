{
  "common": {
    "name": "iobroker.pacs",
    "version": "0.0.1",
    "title": "PACS Adapter",
    "desc": "PACS adapter for user access control",
    "authors": [
      "Mikhail Kolesnikov <m89139158466@gmail.com>"
    ],
    "keywords": [
      "PACS",
      "access control",
      "GSM",
      "users",
      "devices"
    ],
    "license": "MIT",
    "platform": "Javascript/Node.js",
    "icon": "admin/icon.png",
    "enabled": true,
    "extIcon": "https://github.com/mihailkol/iobroker.pacs/blob/master/admin/icon.png",
    "readme": "https://github.com/mihailkol/iobroker.pacs/blob/master/README.md",
    "loglevel": "info",
    "mode": "daemon",
    "type": "communication",
    "connectionType": "local",
    "dataSource": "poll",
    "adminUI": {
      "config": "json"
    },
    "compact": true,
    "messagebox": true,
    "dependencies": [
      {
        "js-controller": ">=5.0.19"
      }
    ],
    "globalDependencies": [
      {
        "admin": ">=6.0.0"
      }
    ],
    "tier": 3
  },
  "native": {
    "users": [],
    "devices": []
  },
  "encryptedNative": [],
  "protectedNative": [],
  "objects": [],
  "instanceObjects": [
    {
      "_id": "info",
      "type": "channel",
      "common": {
        "name": "Information",
        "desc": "Adapter information"
      },
      "native": {}
    },
    {
      "_id": "users",
      "type": "channel",
      "common": {
        "name": "Users",
        "desc": "User management channel"
      },
      "native": {}
    },
    {
      "_id": "users.%USERID%",
      "type": "channel",
      "common": {
        "name": "User",
        "desc": "User channel"
      },
      "native": {}
    },
    {
      "_id": "users.%USERID%.name",
      "type": "state",
      "common": {
        "name": "User name",
        "type": "string",
        "role": "text",
        "read": true,
        "write": true
      },
      "native": {}
    },
    {
      "_id": "users.%USERID%.phone",
      "type": "state",
      "common": {
        "name": "Phone",
        "type": "string",
        "role": "text",
        "read": true,
        "write": true
      },
      "native": {}
    },
    {
      "_id": "users.%USERID%.telegramId",
      "type": "state",
      "common": {
        "name": "Telegram ID",
        "type": "string",
        "role": "text",
        "read": true,
        "write": true
      },
      "native": {}
    },
    {
      "_id": "users.%USERID%.schedule",
      "type": "state",
      "common": {
        "name": "Schedule",
        "type": "string",
        "role": "text",
        "read": true,
        "write": true
      },
      "native": {}
    },
    {
      "_id": "users.%USERID%.isAdmin",
      "type": "state",
      "common": {
        "name": "Is Admin",
        "type": "boolean",
        "role": "indicator",
        "read": true,
        "write": true
      },
      "native": {}
    },
    {
      "_id": "users.%USERID%.devices",
      "type": "state",
      "common": {
        "name": "Devices",
        "type": "string",
        "role": "text",
        "read": true,
        "write": true
      },
      "native": {}
    },
    {
      "_id": "devices",
      "type": "channel",
      "common": {
        "name": "Devices",
        "desc": "Device management channel"
      },
      "native": {}
    },
    {
      "_id": "devices.%DEVICEID%",
      "type": "channel",
      "common": {
        "name": "Device",
        "desc": "Device channel"
      },
      "native": {}
    },
    {
      "_id": "devices.%DEVICEID%.name",
      "type": "state",
      "common": {
        "name": "Device name",
        "type": "string",
        "role": "text",
        "read": true,
        "write": true
      },
      "native": {}
    },
    {
      "_id": "devices.%DEVICEID%.object",
      "type": "state",
      "common": {
        "name": "Object",
        "type": "string",
        "role": "text",
        "read": true,
        "write": true
      },
      "native": {}
    },
    {
      "_id": "devices.%DEVICEID%.command",
      "type": "state",
      "common": {
        "name": "Command",
        "type": "string",
        "role": "text",
        "read": true,
        "write": true
      },
      "native": {}
    }
  ]
}
{
  "type": "tabs",
  "tabsStyle": {
    "width": "calc(100% - 100px)"
  },
  "i18n": true,
  "items": {
    "usersTab": {
      "type": "panel",
      "label": "Пользователи",
      "items": {
        "users": {
          "type": "table",
          "newLine": true,
          "xs": 12,
          "sm": 12,
          "md": 12,
          "lg": 12,
          "xl": 12,
          "label": "Список пользователей",
          "uniqueColumns": [ "name" ],
          "items": [
            {
              "type": "text",
              "title": "Имя",
              "width": "20%",
              "attr": "name",
              "filter": false,
              "sort": false,
              "default": "",
              "placeholder": "Имя пользователя"
            },
            {
              "type": "text",
              "title": "Телефон",
              "width": "15%",
              "attr": "phone",
              "filter": false,
              "sort": false,
              "default": "",
              "placeholder": "Номер телефона"
            },
            {
              "type": "text",
              "title": "Telegram ID",
              "width": "15%",
              "attr": "telegramId",
              "filter": false,
              "sort": false,
              "default": "",
              "placeholder": "ID в Telegram"
            },
            {
              "type": "select",
              "title": "График доступа",
              "width": "15%",
              "attr": "schedule",
              "filter": false,
              "sort": false,
              "default": "working_hours",
              "options": [
                {
                  "value": "working_hours",
                  "label": "Рабочее время"
                },
                {
                  "value": "any_time",
                  "label": "Круглосуточно"
                },
                {
                  "value": "always",
                  "label": "Без ограничений"
                }
              ]
            },
            {
              "type": "checkbox",
              "title": "Администратор",
              "width": "10%",
              "attr": "isAdmin",
              "filter": false,
              "sort": false,
              "default": false
            },
            {
              "type": "text",
              "title": "Устройства",
              "width": "25%",
              "attr": "devices",
              "filter": false,
              "sort": false,
              "default": "",
              "placeholder": "ID устройств"
            }
          ]
        }
      }
    },
    "devicesTab": {
      "type": "panel",
      "label": "Устройства",
      "items": {
        "devices": {
          "type": "table",
          "newLine": true,
          "xs": 12,
          "sm": 12,
          "md": 12,
          "lg": 12,
          "xl": 12,
          "label": "Список устройств",
          "uniqueColumns": [ "name" ],
          "items": [
            {
              "type": "text",
              "title": "Название",
              "width": "30%",
              "attr": "name",
              "filter": false,
              "sort": false,
              "default": "",
              "placeholder": "Название устройства"
            },
            {
              "type": "text",
              "title": "Объект",
              "width": "40%",
              "attr": "object",
              "filter": false,
              "sort": false,
              "default": "",
              "placeholder": "ID объекта"
            },
            {
              "type": "text",
              "title": "Команда",
              "width": "30%",
              "attr": "command",
              "filter": false,
              "sort": false,
              "default": "",
              "placeholder": "Команда управления"
            }
          ]
        }
      }
    },
    "channelsTab": {
      "type": "panel",
      "label": "Входящие каналы",
      "items": {
        "incomingChannels": {
          "type": "table",
          "newLine": true,
          "xs": 12,
          "sm": 12,
          "md": 12,
          "lg": 12,
          "xl": 12,
          "label": "Список каналов",
          "uniqueColumns": [ "name" ],
          "items": [
            {
              "type": "text",
              "title": "Название",
              "width": "30%",
              "attr": "name",
              "filter": false,
              "sort": false,
              "default": "",
              "placeholder": "Название канала"
            },
            {
              "type": "text",
              "title": "Объект",
              "width": "40%",
              "attr": "object",
              "filter": false,
              "sort": false,
              "default": "",
              "placeholder": "Связанный объект"
            },
            {
              "type": "text",
              "title": "Обработка",
              "width": "30%",
              "attr": "processing",
              "filter": false,
              "sort": false,
              "default": "",
              "placeholder": "Тип обработки"
            }
          ]
        }
      }
    }
  }
}
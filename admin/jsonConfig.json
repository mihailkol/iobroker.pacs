{
  "type": "tabs",
  "tabsStyle": {
    "width": "calc(100% - 100px)"
  },
  "i18n": true,
  "items": {
    "usersTab": {
      "type": "panel",
      "label": "Users",
      "items": {
        "users": {
          "type": "table",
          "newLine": true,
          "xs": 12,
          "sm": 12,
          "md": 12,
          "lg": 12,
          "xl": 12,
          "label": "Users",
          "uniqueColumns": [ "name" ],
          "items": [
            {
              "type": "text",
              "title": "Name",
              "width": "20%",
              "attr": "name",
              "filter": false,
              "sort": false,
              "default": "",
              "placeholder": "User name"
            },
            {
              "type": "text",
              "title": "Phone",
              "width": "15%",
              "attr": "phone",
              "filter": false,
              "sort": false,
              "default": "",
              "placeholder": "Phone number"
            },
            {
              "type": "text",
              "title": "Telegram ID",
              "width": "15%",
              "attr": "telegramId",
              "filter": false,
              "sort": false,
              "default": "",
              "placeholder": "Telegram ID"
            },
            {
              "type": "select",
              "title": "Schedule",
              "width": "15%",
              "attr": "schedule",
              "filter": false,
              "sort": false,
              "default": "working_hours",
              "options": [
                {
                  "value": "working_hours",
                  "label": "Working hours"
                },
                {
                  "value": "any_time",
                  "label": "Any time"
                },
                {
                  "value": "always",
                  "label": "Always"
                }
              ]
            },
            {
              "type": "checkbox",
              "title": "Is Admin",
              "width": "10%",
              "attr": "isAdmin",
              "filter": false,
              "sort": false,
              "default": false
            },
            {
              "type": "text",
              "title": "Devices",
              "width": "25%",
              "attr": "devices",
              "filter": false,
              "sort": false,
              "default": "",
              "placeholder": "Device IDs"
            }
          ]
        }
      }
    },
    "devicesTab": {
      "type": "panel",
      "label": "Devices",
      "items": {
        "devices": {
          "type": "table",
          "newLine": true,
          "xs": 12,
          "sm": 12,
          "md": 12,
          "lg": 12,
          "xl": 12,
          "label": "Devices",
          "uniqueColumns": [ "name" ],
          "items": [
            {
              "type": "text",
              "title": "Name",
              "width": "30%",
              "attr": "name",
              "filter": false,
              "sort": false,
              "default": "",
              "placeholder": "Device name"
            },
            {
              "type": "text",
              "title": "Object",
              "width": "40%",
              "attr": "object",
              "filter": false,
              "sort": false,
              "default": "",
              "placeholder": "Object ID"
            },
            {
              "type": "text",
              "title": "Command",
              "width": "30%",
              "attr": "command",
              "filter": false,
              "sort": false,
              "default": "",
              "placeholder": "Command"
            }
          ]
        }
      }
    },
    "channelsTab": {
      "type": "panel",
      "label": "Incoming Channels",
      "items": {
        "incomingChannels": {
          "type": "table",
          "newLine": true,
          "xs": 12,
          "sm": 12,
          "md": 12,
          "lg": 12,
          "xl": 12,
          "label": "Incoming Channels",
          "uniqueColumns": [ "name" ],
          "items": [
            {
              "type": "text",
              "title": "Name",
              "width": "30%",
              "attr": "name",
              "filter": false,
              "sort": false,
              "default": "",
              "placeholder": "Channel name"
            },
            {
              "type": "text",
              "title": "Object",
              "width": "40%",
              "attr": "object",
              "filter": false,
              "sort": false,
              "default": "",
              "placeholder": "Object ID"
            },
            {
              "type": "text",
              "title": "Processing",
              "width": "30%",
              "attr": "processing",
              "filter": false,
              "sort": false,
              "default": "",
              "placeholder": "Processing type"
            }
          ]
        }
      }
    }
  }
}